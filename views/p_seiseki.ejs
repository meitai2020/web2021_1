<!Doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>Show message</title>
</head>
<body>
  <h2>選手成績</h2>
  <table border = 1>
    <th>ID</th>
    <th>名前</th>
    <th>打数</th>
    <th>安打数</th>
    <th>打率</th>
    <th>本塁打</th>
    <th>打点</th>
      <% for( let row of data ) { %>
      <tr>
        <td><%= row.id %></td>
        <td><%= row.player_name%></td>
        <td><%= row.dasuu %></td>
        <td><%= row.hit %></td>
        <td><%= row.ave %></td>
        <td><%= row.homerun %></td>
        <td><%= row.daten %></td>
      </tr>
      <% } %>
  </table>
</body>
</html>